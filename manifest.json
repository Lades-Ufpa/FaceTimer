{
  "name": "FaceCounter",
  "version": "1.3",
  "description": "Facecounter",
  "background": {
    "persistent": false,
    "scripts": [ "background.js", "JQuery-3.2.1.js" ]
  },
  "content_scripts": [
    {
      "matches": [    "<all_urls>",
        "http://*/*",
        "https://*/*",
        "https://*/*" ],
      "js": [ "JQuery-3.2.1.js", "Content.js" ]
    }
  ],
  "browser_action": {
    "default_icon": "camera.png",
    "default_title": "Obrigado por participar!"
  },
  "permissions": [
    "storage",
    "activeTab",
    "webNavigation",
    "tabs",
    "http://*.facebook.com/*", "https://*.facebook.com/*"
  ],
  "manifest_version": 2
}
